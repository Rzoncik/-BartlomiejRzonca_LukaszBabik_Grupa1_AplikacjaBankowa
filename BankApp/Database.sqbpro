    TransferId        INTEGER PRIMARY KEY AUTOINCREMENT,
    SenderUserId      INTEGER  NOT NULL,
    ReceiverUserId    INTEGER  NOT NULL,
    SenderIban        CHAR(28) NOT NULL,
    ReceiverIban      CHAR(28) NOT NULL,
    Amount            DECIMAL(18,2) NOT NULL CHECK (Amount &gt; 0),
    Title             TEXT,
    RequestedAtUtc    TEXT     NOT NULL,   -- ISO 8601
    ExecutedAtUtc     TEXT,
    Status            TEXT     NOT NULL DEFAULT 'Pending',
    RowVersion        BLOB,
    FOREIGN KEY (SenderUserId)   REFERENCES Users(UserId) ON DELETE RESTRICT,
    FOREIGN KEY (ReceiverUserId) REFERENCES Users(UserId) ON DELETE RESTRICT
);</sql><current_tab id="0"/></tab_sql></sqlb_project>
